* {
    background:                  #000000;
    foreground:                  #FDFFFC;
    primary:                     #CFA23D;
    secondary:                   #3E3D23;
    active:                      #9E2A2B;
    active-alt:                  #235789;

    color-border:                  var(active);
}

element {
    padding: 1px ;
    spacing: 5px ;
    border:  0;
    background-color: var(background);
    text-color:       var(foreground);
}

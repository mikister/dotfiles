element-text {
    background-color: rgba ( 0, 0, 0, 0 % );
    highlight:        inherit;
    text-color:       inherit;
}
element-icon {
    background-color: rgba ( 0, 0, 0, 0 % );
    size:             1.0000em ;
    text-color:       inherit;
}

window {
    padding:          20 30;
    background-color: var(background);
    border-color: var(color-border);
    border:           2;
}
mainbox {
    padding: 0;
    border:  0;
}

inputbar {
    padding:    1px ;
    spacing:    0px ;
    text-color: var(foreground);
    /* children:   [ prompt,textbox-prompt-colon,entry,num-filtered-rows,textbox-num-sep,num-rows,case-indicator ]; */

    background-color: var(background);
    text-color:       var(foreground);
    border-color: var(color-border);
    
}
prompt {
    spacing:    0;
    margin: 0px 10px 0px 0px;
    text-color: var(foreground);
    background-color: var(background);
    text-color:       var(foreground);
}
entry {
    text-color:        var(foreground);
    spacing:           0;
    placeholder-color: rgba ( 128, 128, 128, 100 % );
    placeholder:       "Type to filter";
    background-color: var(background);
    text-color:       var(foreground);
}
case-indicator {
    spacing:    0;
    background-color: var(background);
    text-color: var(foreground);
}

message {
    padding:      1px ;
    border-color: var(color-border);
    border:       2px dash 0px 0px ;

    background-color: var(background);
    text-color:       var(foreground);
}
textbox {
    text-color: var(foreground);
}

listview {
    padding:      2px 0px 0px ;
    scrollbar:    false;
    border-color: var(color-border);
    spacing:      2px ;
    fixed-height: 0;
    border:       2px 0px 0px 0px ;
    columns:      1;

    background-color: var(background);
    text-color:       var(foreground);
}
scrollbar {
    width:        4px ;
    padding:      0;
    handle-width: 4px ;
    border:       0;
    handle-color: var(foreground);
}
sidebar {
    border-color: var(color-border);
    border:       2px dash 0px 0px ;
}
button {
    spacing:    0;
    background-color: var(primary);
    text-color: var(foreground);
}
button selected {
    background-color: var(primary);
    text-color:       var(foreground);
}

